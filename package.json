{
  "name": "seed-app",
  "version": "0.0.1",
  "description": "seed app to start work",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha -r ts-node/register \"./test/**/*.spec.ts\" --reporter spec",
    "prestart": "npm run bundle",
    "start": "node ./index.js",
    "startDev": "npm run setDevStart && node -r dotenv/config ./index.js dotenv_config_path=.env.dev",
    "startDevOnMac": "npm run setDevOnMac && node -r dotenv/config ./index.js dotenv_config_path=.env.dev",
    "bundle": "webpack --config ./config/webpack.config.js --mode=production",
    "compile": "webpack --config ./config/webpack.config.js --mode=development",
    "deploy": "gcloud app deploy --quiet",
    "setDevStart": "SET NODE_ENV=development",
    "setDevOnMac": "export NODE_ENV=development"
  },
  "repository": "seed repo",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "axios-retry": "^3.1.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "error-to-json": "^1.1.0",
    "express": "^4.16.4",
    "express-correlation-id": "^1.3.0",
    "joi": "^14.3.1",
    "node-pre-gyp": "^0.13.0",
    "path-to-regexp": "^3.0.0",
    "request": "^2.87.0",
    "url-parse": "^1.4.6",
    "uuid4": "^1.1.4"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.3.0",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.7",
    "@types/dateformat": "^3.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/sinon-chai": "^3.2.3",
    "@types/sinon-express-mock": "^1.3.7",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "sinon-chai": "^3.4.0",
    "sinon-express-mock": "^2.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  }
}
